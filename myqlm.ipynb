    "from qat.lang import qrout, H, CNOT\n",
    "\n",
    "@qrout\n",
    "def bell_pair():\n",
    "    H(0)\n",
    "    CNOT(0, 1)\n",
    "\n",
    "result = bell_pair().run()\n",
    "\n",
    "for sample in result:\n",
    "    print(f\"State {sample.state} amplitude {sample.amplitude}\")"
